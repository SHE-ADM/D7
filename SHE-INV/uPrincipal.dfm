  object QEstoqueFardo: TIBQuery
    Database = FBird.DBERP
    Transaction = TConsulta
    SQL.Strings = (
      
        'SELECT   PK.NCM,PK.UCOM,MAX(PK.DTNF) AS DTNF,CAST(DATEDIFF(YEAR ' +
        'FROM MAX(PK.DTNF) TO CURRENT_DATE) AS SMALLINT)  AS QANF,'
      
        '         MAX(IIF(PK.QTDE > 0,PK.XPROD,NULL)) AS XPROD,AVG(PK.VUN' +
        'COM) AS VUNCOM,SUM(PK.QTDE) AS QTDE,SUM(PK.QTDS) AS QTDS,SUM(PK.' +
        'QTDE-PK.QTDS) AS EPE_QTDE'
      'FROM     VW_CAD_PRO_EST_INV_FAR AS PK'
      'WHERE    PK.IS_SPED = 1'
      
        'AND      NOT EXISTS (SELECT ID FROM CAD_PRO AS CP WHERE CP.NCM =' +
        ' PK.NCM AND CP.IDEP <> 5)'
      'GROUP BY 1,2'
      'HAVING   SUM(PK.QTDE-PK.QTDS) > 0'
      'ORDER BY PK.NCM')
    Left = 8
    Top = 72
