  object QEstoque: TIBQuery
    Database = FBird.DBERP
    Transaction = TConsulta
    SQL.Strings = (
      'SELECT   CP.NCM,CP.UCOM,CP.ORIG,'
      '         MAX(CP.DECP) AS DECP,AVG(CP.VPRC_COM_PAD) AS VPRC,'
      
        '         COALESCE(SUM(EK.EPE_QTDE),0) AS EPE_QTDE,COALESCE(SUM(E' +
        'K.EPE_QTDE*CP.UPSLQ),0) AS EPE_PESO'
      'FROM     VW_CAD_PRO AS CP'
      'JOIN     SP_CAD_PRO_EST_PSQ(:IDEP,CP.IDEK) AS EK ON (1=1)'
      'WHERE    CP.IDEP = :IDEP'
      'AND      CP.REST = '#39'A'#39
      'AND      CP.NCM <> '#39'00000000'#39
      'AND      NOT FEMPTY(CP.NCM)'
      'GROUP BY NCM,UCOM,ORIG'
      'HAVING   SUM(EK.EPE_QTDE) > 0')
    Left = 8
    Top = 104
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'IDEP'
        ParamType = ptUnknown
